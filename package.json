{
  "name": "project-name",
  "version": "0.0.1",
  "description": "project description",
  "scripts": {
    "build:perf": "export NODE_ENV=performance; webpack --config webpack.prod.ts --progress",
    "build": "export NODE_ENV=production; webpack --config webpack.prod.ts --progress",
    "check:format": "prettier --check --config ./.prettierrc \"src/**/*.(ts|tsx)\"",
    "check:imports": "unimported",
    "check:lint-staged": "lint-staged",
    "check:lint": "eslint ./src",
    "check:types": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "fix:format": "npm run test:format -- --write",
    "fix:lint": "npm run test:lint -- --fix",
    "serve": "serve ./dist",
    "start": "webpack serve --config webpack.dev.ts --progress --mode=development",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test": "ENV=test jest"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.5.1"
  },
  "lint-staged": {
    "*.md": "prettier --list-different",
    "*.{ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix"
    ]
  },
  "author": "Patryk Lizo≈Ñ",
  "license": "ISC",
