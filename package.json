    "@types/chai": "^4.3.1",
    "@types/chai-dom": "^0.0.13",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.7",
    "browserslist": "^4.20.3",
    "chai": "^4.3.6",
